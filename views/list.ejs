<%- include("header")-%>
    <div id="heading" class="box">
        <h1>Its <%=listTitle%>!</h1>
    </div>

    <div class="box">
        <%for(let step = 0; step < newListItem.length; step++){%>
            <div class="item">
                <input type="checkbox">
                <p><%=newListItem.at(step)%></p>
            </div>
        <%}%>
        <form class="item" action="/" method="post">
            <input type="text" name="item" placeholder="New Item" autocomplete="off">
            <button name="inputItem" type="submit" value=<%=listTitle%>>+</button>
        </form>
    </div>
<%- include("footer")-%>